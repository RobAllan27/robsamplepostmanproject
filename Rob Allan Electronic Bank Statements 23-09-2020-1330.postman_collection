									"\r",
									"pm.test(\"Get Application - Content-Type header has a specific value - \\\"application/json; charset=UTF-8\\\"\", function () {\r",
									"    pm.response.to.be.header(\"Content-Type\",\"application/json\");\r",
									"});\r",
									"\r",
