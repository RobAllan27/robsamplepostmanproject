											" * @param {array} secondArray\r",
											" *\r",
											" * @returns {boolean}\r",
											" */\r",
											"function arraysSameElements(firstArray, secondArray) {\r",
											"  if (firstArray.length !== secondArray.length) {\r",
											"    return false;\r",
											"  }\r",
											"  return secondArray.every(function (value) {\r",
											"    return (firstArray.indexOf(value) >= 0);\r",
											"  });\r",
											"}"
